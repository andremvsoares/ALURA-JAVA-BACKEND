// Gerente herda da class Funcionario
public class Gerente extends Funcionario {
	
	private String nome;
	private String cpf;
	private double salario;
	private int senha;
		
	
	public boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		}else {
			return false;
		}
	}
	
//	public double getBonificacao() {
//		return this.salario;
//	}
		
	
	
	
}
